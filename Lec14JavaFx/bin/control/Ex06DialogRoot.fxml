<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.HBox?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>

<HBox xmlns:fx="http://javafx.com/fxml/1"
		fx:controller="control.Ex06DialogRootController"
		spacing="10">

	<padding>
		<Insets topRightBottomLeft="10"/>
	</padding>

	<children>
		<Button text="Open File" onAction="#handleOpenFile"/>
		<Button text="Save File" onAction="#handleSaveFile"/>
		<Button text="Open Directory" onAction="#handleOpenDirectory"/>
		<Button text="Popup" onAction="#handlePopup"/>
		<Button text="Custom" onAction="#handleCustom"/>
	</children>

</HBox>

